{
  "name": "wp-control",
  "version": "0.2.0",
  "description": "Helps you navigate between all your WordPress websites.",
  "repository": "https://github.com/123code-il/wp-control",
  "author": "Idan Goldman <idan@123code.co>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -frv .dist .parcel-cache node_modules pnpm-lock.yaml",
    "watch": "rm -frv .dist && parcel watch ./src/manifest.json --dist-dir=.dist --host localhost"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=8.12.1"
  },
  "parcelIgnore": [
    "{{.*}}"
  ],
  "devDependencies": {
    "@fontsource/roboto": "^5.0.8",
    "@fontsource/roboto-mono": "^5.0.16",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo": "8.0.0-beta.2",
    "@parcel/compressor-gzip": "^2.10.3",
    "@parcel/config-webextension": "^2.10.3",
    "@parcel/transformer-inline-string": "^2.10.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "add": "^2.0.6",
    "chokidar": "^3.5.3",
    "cssnano": "^6.0.2",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "parcel": "^2.10.3",
    "parcel-resolver-ignore": "^2.2.0",
    "postcss": "^8.4.32",
    "postcss-apply": "^0.12.0",
    "postcss-atroot": "^0.2.4",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^15.1.0",
    "postcss-import-url": "^7.2.0",
    "postcss-url": "^10.1.3",
    "posthtml": "^0.16.6",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^1.7.4",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.0"
  }
}
