{
  "name": "wp-control",
  "version": "0.0.1",
  "description": "Helps you navigate between all your WordPress websites.",
  "repository": "https://github.com/123code-il/wp-control",
  "author": "Idan Goldman <idan@123code.co>",
  "license": "MIT",
  "scripts": {
    "watch": "rm -fr .dist && export NODE_ENV=development && parcel watch ./src/manifest.json --dist-dir=.dist --host localhost --config @parcel/config-webextension",
    "build": "rm -fr .dist && export NODE_ENV=development && parcel build ./src/manifest.json --dist-dir=.dist --config @parcel/config-webextension"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@parcel/config-webextension": "^2.6.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "chokidar": "^3.5.3",
    "cssnano": "^5.1.9",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "parcel": "^2.6.0",
    "postcss": "^8.2.15",
    "postcss-apply": "^0.12.0",
    "postcss-atroot": "^0.2.3",
    "postcss-extend-rule": "^4.0.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.24"
  }
}
