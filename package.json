{
  "name": "wp-control",
  "version": "0.0.1",
  "description": "Helps you navigate between all your WordPress websites.",
  "repository": "https://github.com/123code-il/wp-control",
  "author": "Idan Goldman <idan@123code.co>",
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch ./src/manifest.json --dist-dir=.dist --host localhost --config @parcel/config-webextension",
    "build": "parcel build ./src/manifest.json --dist-dir=.dist --config @parcel/config-webextension"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.6.0",
    "parcel": "^2.6.0",
    "tailwindcss": "^3.0.24"
  }
}
